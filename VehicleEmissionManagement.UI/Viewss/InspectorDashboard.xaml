<Window x:Class="VehicleEmissionManagement.UI.Viewss.InspectorDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Inspector Dashboard" Height="600" Width="800">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Left" Background="#e67e22" Width="200">
                <TextBlock Text="Inspector Dashboard" Foreground="White" FontSize="20" Margin="20,20,20,40"/>
                <Button Content="Today's Inspections" Margin="10,5" Height="40"/>
                <Button Content="Record Results" Margin="10,5" Height="40"/>
                <Button Content="View History" Margin="10,5" Height="40"/>
                <Button Content="Equipment Status" Margin="10,5" Height="40"/>
            </StackPanel>
            <Grid Background="#ecf0f1">
                <TextBlock Text="Welcome to Inspector Dashboard" 
                         FontSize="24" HorizontalAlignment="Center" 
                         VerticalAlignment="Top" Margin="0,20,0,0"/>
            </Grid>
        </DockPanel>
    </Grid>
</Window>
